{
    "manifest_version": 3,
    "name": "Resume Tracker",
    "version": "1.0",
    "description": "Track which resume you used for each job application",
    "permissions": ["storage", "activeTab", "tabs", "downloads"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
  